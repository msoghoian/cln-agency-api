{
  "name": "cln-agency-api",
  "version": "0.1.0",
  "bin": {
    "cln-agency-api": "bin/cln-agency-api.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "npx eslint .",
    "aws:sso:login:dev": "aws sso login --profile cln-dev_sso",
    "cdk": "cdk",
    "cdk:bootstrap:dev": "cdk bootstrap -c stage=dev --profile cln-dev_sso",
    "cdk:synth:dev": "cdk synth -c stage=dev",
    "cdk:deploy:dev": "cdk deploy -c stage=dev --profile cln-dev_sso",
    "cdk:destroy:dev": "cdk destroy -c stage=dev --profile cln-dev_sso"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.18.0",
    "aws-cdk": "2.1028.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "jiti": "^2.5.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.879.0",
    "@aws-sdk/s3-request-presigner": "^3.879.0",
    "aws-cdk-lib": "2.214.0",
    "constructs": "^10.4.2"
  }
}
